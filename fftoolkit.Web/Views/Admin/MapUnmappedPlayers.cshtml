@model List<fftoolkit.DB.Model.Player>

@{
    ViewBag.Title = "MapUnmappedPlayers";
}

<h2>Map Unmapped Players</h2>

@Html.BeginForm("MapUnmappedPlayers", "Admin", FormMethod.Post)
{
    @for (int i = 0; i < Model.Count(); i++)
    {
        <div class="row">
            @Model[i].Name (@Model[i].Position, @Model[i].Team)
        </div>
    }

    <input type="submit" value="Create Mapping" class="btn btn-primary" />
}